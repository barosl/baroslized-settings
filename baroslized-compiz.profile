[gnomecompat]
as_run_key = <Mod4>r
as_run_command_terminal_key = <Mod4>t

[put]
as_put_left_key = Disabled
as_put_right_key = Disabled
as_put_top_key = Disabled
as_put_bottom_key = Disabled
as_put_topleft_key = Disabled
as_put_topright_key = Disabled
as_put_bottomleft_key = Disabled
as_put_bottomright_key = Disabled

[wobbly]
as_snap_inverted = true

[staticswitcher]
s0_popup_delay = 0.000000

[rotate]
as_rotate_left_key = <Mod4>Left
as_rotate_right_key = <Mod4>Right
as_rotate_left_window_key = <Mod4><Alt>Left
as_rotate_right_window_key = <Mod4><Alt>Right

[extrawm]
as_toggle_always_on_top_key = <Super>g

[fade]
s0_fullscreen_visual_bell = true

[core]
as_active_plugins = core;ccp;move;resize;place;decoration;vpswitch;imgjpeg;gnomecompat;commands;extrawm;session;annotate;svg;dbus;regex;png;text;workarounds;mousepoll;wall;wobbly;thumbnail;obs;resizeinfo;animation;fade;expo;scale;ezoom;scaleaddon;staticswitcher;
as_cursor_theme = DMZ-White
as_autoraise = false
as_autoraise_delay = 500
as_close_window_key = <Mod4>space
as_minimize_window_key = <Mod4>n
as_maximize_window_key = Disabled
as_show_desktop_key = <Super>d
as_toggle_window_maximized_key = <Mod4>m
as_toggle_window_shaded_key = Disabled
s0_hsize = 2
s0_vsize = 2
s0_outputs = 1366x768+0+0;

[scale]
as_initiate_key = F4

[expo]
as_expo_key = <Super>backslash

[wall]
as_left_key = <Mod4>Left
as_right_key = <Mod4>Right
as_up_key = <Mod4>Up
as_down_key = <Mod4>Down
as_left_window_key = <Mod4><Alt>Left
as_right_window_key = <Mod4><Alt>Right
as_up_window_key = <Mod4><Alt>Up
as_down_window_key = <Mod4><Alt>Down

[ezoom]
as_zoom_specific_1_key = <Super>End
as_lock_zoom_key = <Super>Delete
as_fit_to_window_key = <Super>Home

[commands]
as_command0 = leafpad
as_command1 = gvim
as_run_command0_key = <Super>e
as_run_command1_key = <Super>v

[decoration]
as_shadow_radius = 9.000000
as_command = /usr/bin/emerald
as_shadow_match = !(name=gnome-panel)

