# for FreeNX
export ESPEAKER=127.0.0.1:$((`echo $DISPLAY | cut -d: -f2 | cut -d. -f1`+7000))

# for make
export MAKEFLAGS='-j5'

# Java classpath
export CLASSPATH='.:/usr/share/java/qtjambi-4.7.4.jar'

# Ubuntu-specific settings
#export LIBOVERLAY_SCROLLBAR=0

# Restoring the historical umask
umask 022

EOF

