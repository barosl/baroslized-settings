# vim: set ft=conf: #

remote barosl.com 1194

ca keys/ca.crt
cert keys/client.crt
key keys/client.key

dev tun
proto udp
comp-lzo
persist-key
persist-tun
keepalive 10 60

client
nobind
resolv-retry infinite
ns-cert-type server

#user nobody
#group nogroup
#chroot /tmp

verb 3

script-security 2
up .client-up.bat
down .client-down.bat
