# vim: set ft=conf: #

port 1194
proto udp

ca keys/ca.crt
cert keys/server.crt
key keys/server.key
dh keys/dh1024.pem

dev tun
server 10.8.8.0 255.255.255.0
keepalive 10 120
comp-lzo
persist-key
persist-tun

client-to-client

user nobody
group nogroup
chroot /tmp

ifconfig-pool-persist logs/ipp.txt
status logs/status.txt
log-append logs/openvpn.log
verb 3
